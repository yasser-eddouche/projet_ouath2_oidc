<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 
        OWASP Dependency-Check Suppression File
        
        Suppressed vulnerabilities should be documented with:
        - Reason for suppression
        - Expected resolution date
        - Mitigation measures in place
    -->
    
    <!-- Example suppression - Remove or modify as needed
    <suppress>
        <notes><![CDATA[
            False positive - This vulnerability does not affect our usage
        ]]></notes>
        <cve>CVE-2023-XXXXX</cve>
    </suppress>
    -->
    
    <!-- Suppress known false positives for Spring Boot -->
    <suppress>
        <notes><![CDATA[
            Spring Boot Actuator endpoints are secured via Spring Security
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring\-boot\-actuator.*$</packageUrl>
        <cpe>cpe:/a:vmware:spring_boot</cpe>
    </suppress>
    
    <!-- Suppress development-only dependencies -->
    <suppress>
        <notes><![CDATA[
            Test dependencies not used in production
        ]]></notes>
        <gav regex="true">^org\.springframework\.boot:spring\-boot\-starter\-test:.*$</gav>
    </suppress>
</suppressions>
